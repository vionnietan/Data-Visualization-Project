{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 1000,
  "height": 100,
  "data": {
    "url": "https://raw.githubusercontent.com/vionnietan/FIT3179/master/Assignment%202/data/linechart_final.csv"
  },
  "mark": "rect",
  "encoding": {
    "x": {
      "timeUnit": "year",
      "field": "year",
      "type": "ordinal",
      "title": "Year",
      "axis": {
        "format": "%Y",
        "labelAngle": 0,
        "labelOverlap": false,
        "labelColor": {
          "condition": {
            "test": {
              "timeUnit": "monthdate",
              "field": "value",
              "equal": {"month": 1, "date": 1}
            },
            "value": "black"
          },
          "value": null
        },
        "tickColor": {
          "condition": {
            "test": {
              "timeUnit": "monthdate",
              "field": "value",
              "equal": {"month": 1, "date": 1}
            },
            "value": "black"
          },
          "value": null
        }
      }
    },
    "color": {
      "aggregate": "sum",
      "field": "suicides_no",
      "type": "quantitative",
      "title": "Avg Suicide Rate"
    },
    "y": {"field": "age", "type": "nominal", "title": null}
  },
  "config": {}
}