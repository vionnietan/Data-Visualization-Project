{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "background":"#FEF8EC",
  "width":"container",
  "height":250,
  "data": {"url": "https://raw.githubusercontent.com/vionnietan/FIT3179/master/Assignment%202/data/top10.csv"},
  "mark": "bar",
  "encoding": {
    "y": {
      "field": "country",
      "type": "ordinal",
      "title":null
    },
    "x": {
      "field":"rate_per_100k",
      "aggregate": "mean",
      "type": "quantitative",
      "title":"Avg per 100k"
    },
    "color": {
      "field": "age",
      "type": "nominal",
      "scale": {
        "domain": ["05-14", "15-24", "25-34", "35-54", "55-74", "75+"],
        "range": ["#e7ba52", "#c7c7c7", "#aec7e8", "#1f77b4", "#54278f","#9467bd"]
      },
      "title": "Age Group"
    },
    "tooltip": [
      {"field": "age", "type": "nominal","title":"Age Group"},
      {
        "field":"rate_per_100k",
        "aggregate": "mean",
        "type": "quantitative",
        "title": "Average Suicide Rate",
        "format":".2f"
      }
    ]
  }
}
