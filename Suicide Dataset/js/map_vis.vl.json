{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "Suicide Count in Asia",
  "width": 500,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/vionnietan/FIT3179/master/Suicide%20Dataset/partmap.topo.json",
    "format": {"type": "topojson", "feature": "partmap"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/vionnietan/FIT3179/master/Suicide%20Dataset/suicide_count.csv"
        },
        "key": "Country",
        "fields": ["Count"]
      }
    }
  ],
  "projection": {"type": "equalEarth"},
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Count"
    }
  }
}
